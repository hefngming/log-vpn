{
  "name": "log-vpn"
  "version": "1.0.6"
  "type": "module"
  "main": "dist_electron/main.js"
   "scripts": {
    "dev": "vite"
    "build": "vite build && npx tsc -p tsconfig.electron.json --noEmit && electron-builder"
    "preview": "vite preview"
  }
  "build": {
    "appId": "com.logvpn.app"
    "productName": "LogVPN"
    "directories": {
      "output": "release"
    }
    "files": [
      "dist/**/*",
      "dist_electron/**/*",
      "resources/**/*",
      "electron/**/*"
    ]
    "win": {
      "target": "portable"
      "artifactName": "LogVPN-Setup.${ext}"
    }
  }
