{
  "name": "log-vpn",
  "version": "1.0.6",
  "type": "module",
  "main": "dist_electron/main.js",
  "scripts": {
    "dev": "vite",
    "build": "vite build && npx tsc -p tsconfig.electron.json --noEmit && electron-builder",
    "preview": "vite preview"
  },
  "dependencies": {
    "vue": "^3.3.4"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.2.3"
    "electron": "^25.3.0",
    "electron-builder": "^24.6.3",
    "typescript": "^5.0.2",
    "vite": "^4.4.5",
    "vue-tsc": "^1.8.5"
  },
  "build": {
    "appId": "com.logvpn.app",
    "productName": "LogVPN",
    "directories": {
      "output": "release"
    },
    "files": [
      "dist/**/*",
      "dist_electron/**/*",
      "resources/**/*",
      "electron/**/*"
    ],
    "win": {
      "target": "portable",
      "artifactName": "LogVPN-Setup.${ext}"
    }
  }
}
