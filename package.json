{
  "name": "log-vpn",
  "version": "1.0.6",
  "type": "module",
  "license": "MIT",
  "main": "dist_electron/main.js",
  "scripts": {
    "build": "vite build && esbuild client-src/main.ts --bundle --platform=node --external:electron --format=esm --target=node18 --outfile=dist_electron/main.js",
    "package": "npm run build && electron-builder --win --publish never"
  },
  "dependencies": {
    "react": "^19.2.1",
    "react-dom": "^19.2.1"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^5.1.2",
    "vite": "^7.3.1",
    "electron": "^30.5.1",
    "electron-builder": "^25.1.8",
    "esbuild": "^0.25.12"
  },
  "build": {
    "appId": "com.logvpn.app",
    "productName": "LogVPN",
    "directories": {
      "output": "release"
    },
    "files": [
      "dist/**/*",
      "dist_electron/**/*"
    ],
    "win": {
      "target": ["portable"],
      "icon": "resources/icon.ico"
    }
  }
}
