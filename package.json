{
  "name": "log-vpn",
  "version": "1.0.6",
  "type": "module",
  "main": "dist_electron/main.js",
  "scripts": {
    "dev": "vite",
    "build": "vite build && tsc -p tsconfig.electron.json && electron-builder",
    "preview": "vite preview"
  },
  "build": {
    "appId": "com.logvpn.app",
    "productName": "LogVPN",
    "directories": {
      "output": "release",
      "buildResources": "resources"
    },
    "files": [
      "dist/**/*",
      "dist_electron/**/*",
      "resources/**/*"
    ],
    "win": {
      "target": "portable"
      "artifactName": "${productName}-Setup-${version}.${ext}"
    }
  }
}
