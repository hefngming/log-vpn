{
  "apiConfig": {
    "baseUrl": "https://your-domain.com",
    "apiPath": "/api/trpc",
    "oauthPath": "/api/oauth/authorize",
    "endpoints": {
      "nodes": {
        "list": "/nodes.list",
        "getEncrypted": "/nodes.getEncrypted"
      },
      "traffic": {
        "log": "/traffic.log"
      },
      "auth": {
        "me": "/auth.me",
        "logout": "/auth.logout"
      },
      "version": {
        "check": "/version.check",
        "latest": "/version.latest"
      }
    }
  },
  "encryption": {
    "algorithm": "AES-256-CBC",
    "key": "your-32-byte-encryption-key-here",
    "note": "此密钥必须与服务器端 server/nodeEncryption.ts 中的 ENCRYPTION_KEY 完全一致"
  },
  "clientConfig": {
    "appName": "LogVPN",
    "version": "1.0.0",
    "updateCheckInterval": 3600,
    "nodeRefreshInterval": 300,
    "trafficReportInterval": 300,
    "autoConnect": true,
    "autoSelectBestNode": true
  },
  "deviceFingerprint": {
    "enabled": true,
    "components": [
      "cpuId",
      "diskSerial",
      "macAddress",
      "motherboardSerial"
    ],
    "note": "用于免费试用限制和防止账号共享"
  }
}
